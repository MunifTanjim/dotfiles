{
  "local": {
    "prefix": "l",
    "body": ["local ${0}"],
    "description": "local variable"
  },
  "require": {
    "prefix": "rq",
    "body": ["require(\"${1:mod}\")"],
    "description": "require module"
  },
  "local_require": {
    "prefix": "lrq",
    "body": ["local ${1:var} = require(\"${2:mod}\")"],
    "description": "require module as local variable"
  },
  "function": {
    "prefix": ["fn"],
    "body": ["function ${1:fn_name}(${2})", "  ${3}", "end"],
    "description": "function"
  },
  "method_function": {
    "prefix": ["mfn"],
    "body": ["function ${1:class_name}:${2:fn_name}(${3})", "  ${4}", "end"],
    "description": "method function"
  },
  "local_function": {
    "prefix": ["lfn"],
    "body": ["local function ${1:fn_name}(${2})", "  ${3}", "end"],
    "description": "local function"
  },
  "print": {
    "prefix": "p",
    "body": ["print(${0})"],
    "description": "print"
  },
  "print_vim_inspect": {
    "prefix": "pvi",
    "body": ["print(vim.inspect(${0}))"],
    "description": "print vim.inspect"
  },
  "test_describe": {
    "prefix": "td",
    "body": ["describe(\"$1\", function()", "  $0", "end)"],
    "description": "test describe block"
  },
  "test_it": {
    "prefix": "ti",
    "body": ["it(\"$1\", function()", "  $0", "end)"],
    "description": "test it block"
  },
  "test_before_each": {
    "prefix": "tbe",
    "body": ["before_each(function()", "  $0", "end)"],
    "description": "test before_each block"
  },
  "test_after_each": {
    "prefix": "tae",
    "body": ["after_each(function()", "  $0", "end)"],
    "description": "test after_each block"
  }
}
