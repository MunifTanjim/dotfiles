{
  "const": {
    "prefix": "con",
    "body": "const ${1:name} = ${0:null}"
  },
  "let": {
    "prefix": "let",
    "body": "let ${1:n} = ${0:null}"
  },
  "export": {
    "prefix": "exp",
    "body": "export "
  },
  "export_default": {
    "prefix": "expd",
    "body": "export default "
  },
  "export_from": {
    "prefix": "expf",
    "body": "export ${2} from '${1}'"
  },
  "forof": {
    "prefix": "forof",
    "body": ["for (const ${1:item} of ${2:${1}s}) {", "  $3", "}"]
  },
  "import_from": {
    "prefix": "impf",
    "body": ["import ${2} from '${1}'"]
  },
  "import": {
    "prefix": "imp",
    "body": ["import '${1}'"]
  },
  "console_log": {
    "prefix": "cl",
    "body": "console.log(${1:null})"
  },
  "console_log_json_stringify": {
    "prefix": "cljs",
    "body": "console.log(JSON.stringify(${1:null}, null, 2))"
  },
  "console_log_variable": {
    "prefix": "clv",
    "body": "console.log('${1:null}', $1)"
  },
  "function": {
    "prefix": "fn",
    "body": ["function ${1:name}($2) {", "  $3", "}"]
  },
  "export_function": {
    "prefix": "efn",
    "body": ["export function ${1:name}($2) {", "  $3", "}"]
  },
  "export_default_function": {
    "prefix": "edfn",
    "body": ["export default function ${1:name}($2) {", "  $3", "}"]
  },
  "arrow_function": {
    "prefix": "afn",
    "body": ["($1) => {", "  $2", "}"]
  },
  "try_catch": {
    "prefix": "tryc",
    "body": ["try {", "  $0", "} catch (${1:err}) {", "  $2", "}"]
  },
  "try_catch_finally": {
    "prefix": "trycf",
    "body": [
      "try {",
      "  $0",
      "} catch (${1:err}) {",
      "  $2",
      "} finally {",
      "  $3",
      "}"
    ]
  },
  "try_finally": {
    "prefix": "tryf",
    "body": ["try {", "  $0", "} finally {", "  $1", "}"]
  },
  "test_after_each": {
    "prefix": "tae",
    "body": ["afterEach(async () => {", "  $0", "})"]
  },
  "test_before_each": {
    "prefix": "tbe",
    "body": ["beforeEach(async () => {", "  $0", "})"]
  },
  "test_describe": {
    "prefix": "td",
    "body": ["describe('${1:name}', () => {", "  $0", "})"]
  },
  "test_it": {
    "prefix": "ti",
    "body": ["it('${1:description}', async () => {", "  $0", "})"]
  }
}
